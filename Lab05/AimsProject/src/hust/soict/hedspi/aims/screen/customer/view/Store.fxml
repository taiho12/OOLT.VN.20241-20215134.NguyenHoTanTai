<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/11.0.14-internal"
      xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets left="20.0"/>
    </padding>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
        <Label prefHeight="200.0" prefWidth="200.0" text="AIMS" textFill="#004cff">
            <font>
                <Font size="50.0"/>
            </font>
            <padding>
                <Insets left="10.0"/>
            </padding>
        </Label>
        <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS"/>
        <Button minHeight="50.0" mnemonicParsing="false" prefWidth="100.0"
                onAction="#btnViewCartPressed" text="View Cart">
            <HBox.margin>
                <Insets right="20.0"/>
            </HBox.margin>
        </Button>
    </HBox>
    <ScrollPane prefHeight="760.0" prefWidth="1024.0">
        <GridPane fx:id="gridPane" prefHeight="0.0" prefWidth="0.0">
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
    </ScrollPane>
</VBox>
